{
  "name": "edmondscommerce/spreadsheet-to-csv",
  "license": "MIT",
  "type": "library",
  "description": "A library for converting multi worksheet spreadsheets into CSVs",
  "autoload": {
    "psr-4": {
      "EdmondsCommerce\\SpreadsheetToCsv\\": [
        "src/"
      ]
    }
  },
  "autoload-dev": {
    "psr-4": {
      "EdmondsCommerce\\SpreadsheetToCsv\\Tests\\": [
        "tests"
      ]
    }
  },
  "require": {
    "php": ">=7.2",
    "phpoffice/phpspreadsheet": "^1.4"
  },
  "require-dev": {
    "edmondscommerce/phpqa": "^1.2",
    "roave/security-advisories": "dev-master@dev"
  },
  "config": {
    "bin-dir": "bin",
    "preferred-install": {
      "edmondscommerce/*": "source",
      "*": "dist"
    },
    "optimize-autoloader": true,
    "github-protocols": [
      "https"
    ]
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
